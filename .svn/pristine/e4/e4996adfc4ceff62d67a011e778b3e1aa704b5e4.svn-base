body {
    background: #f5f5f5;;
    color: #333;
    font-size: 16px;
    margin-bottom: 80px;
}

.mui-content {
    background-color: #f5f5f5;
}

a {
    color: #333;
}

.red {
    color: #ff0000;
}

.mui-bar {
    background: #fff;
}

.user {
    height: auto;
    margin: 0 auto;
    position: relative;
    font-family: "微软雅黑";
}

.web-top-right {
    position: absolute;
    top: 0;
    right: 10px;
    font-size: 1rem;
    height: 100%;
    line-height: 44px;
}

.web-top-right a {
    display: inline-block;
    width: 20px;
    height: 20px;
    margin-top: 13px;
}

.web-top-right span {
    margin-right: 5px;
}

.user .msg-icon {
    width: 100%;
    height: 100%;
}

.user .personal {
    position: relative;
    width: 100%;
    margin-top: 10px;
    padding: 10px;
    background-size: 100% 100%;
    border-radius: 6px;
    color: #fff;
    background: -webkit-linear-gradient(left,#eb3c3c,#ff7459);
    background: linear-gradient(90deg,#eb3c3c,#ff7459);
    box-shadow: 0 2px 5px rgba(255,98,98,.4);
}

.user .personal .number {
    float: left;
    margin-left: 15px;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    min-width: 0;
    line-height: 80px;
}
.user  .my-a-icon{
    position: absolute;
    top: 12px;
    right: 15px;
    padding-left: 18px;
    color: rgba(76,0,0,.7);
    background-size: 13px;
    font-size: 12px;
}
.user-main-list {
    background: #FFF;
    border-top: 1px solid #E5E5E5;
    margin-top: 1rem;
}

.user-main-list li {
    width: 100%;
    height: 60px;
    line-height: 50px;
    border-bottom: 1px solid #E5E5E5;
    background: url(../img/icon_arrow.png) no-repeat 98% center;
    background-size: .5rem;
}

.user-main-list li:nth-child(2) {
    border-bottom: 0;
}

.user-main-list li a {
    padding: 5px;
    display: block;
    width: 100%;
    height: 100%;
}

.user-main-list img {
    width: 24px;
    height: 27px;
    margin: 12px 15px 0 0;
}

.viewOrder {
    color: #999;
    margin-right: 20px;
}

.order-item {
    overflow: hidden;
}

.order-box {
    width: 25%;
    text-align: center;
    padding: 10px 0;
    float: left;
}

.order-box a {
    display: block;
}

.order-box span {
    display: block;
    margin-top: 5px;
    font-size: 14px;
}

.order-box img {
    width: 30px;
    height: 30px;
    margin: 0;
}

.quit {
    padding: 20px 20px 0 20px;
}

.quit_btn {
    width: 80%;
    height: 40px;
    background: #30a5dd;
    border-radius: 5px;
    text-align: center;
    line-height: 40px;
    display: block;
    margin: 0 auto;
    color: #fff;
    font-size: 16px;
    margin-bottom: 50px;
    padding: 0;
}

/* 个人中心 end */

/* 我的订单 start */

.order #news-tab {
    background: #fff;
    overflow: hidden;
}

.order #news-tab .swiper-slide:first-child {
    margin-left: 4px;
}

.order #news-tab .swiper-slide {
    width: 20%;
    padding: 10px 0;
    margin: 0;
    text-align: center;
    color: #212121;
}

.order .tabs {
    height: 42px;
    background: #fff;
    border-top: 1px solid #D7D7D7;
    font-size: 14px;
    font-weight: bold;
}

.order ul li {
    width: 20%;
    float: left;
    list-style: none;
    text-align: center;
    padding: 10px 0;
}

.order .active {
    color: #ff0000;
    border-bottom: 1px solid #FF0000;
}

.order .goods-item {
    background: #FFF;
    margin-bottom: 10px;
}

.order .goods-item .head {
    padding: 10px;
    color: #212121;
    border-bottom: 1px solid #E4E4E4;
    border-top: 1px solid #E4E4E4;
    font-size: 14px;
}

.pingjia {
    padding: 10px;
    color: #212121;
    text-align: center;
    border-top: 1px solid #E4E4E4;
}

.pingjia span {
    margin-right: 10px
}

.order .goods-layout {
    width: 100%;
    padding: 10px 0;
}

.goods-layout {
    background: #FFF;
    border-bottom: 1px solid #E4E4E4;
    margin-bottom: 10px;

}

.order .goods-img {
    padding-left: 10px;
    width: 75%;
    float: left;
}

.order .goods-img img {
    width: 100px;
    height: 100px;
    margin-right: 5px;
    border: 1px solid #CCCCCC;
    display: inline-block;

}

.order .goods-right {
    width: 25%;
    text-align: right;
    font-size: 12px;
    padding-right: 10px;
    float: left;

}

.goods-right span {
    display: block;
    height: 20px;
    margin: 13px 0px 0 0;
}

.order .foot {
    padding: 10px;
    font-size: 14px;
}

.order .foot .fl {
    line-height: 30px;
}

.order .order-list-btn {
    color: #fff;
    display: inline-block;
    width: 68px;
    height: 27px;
    border-radius: 3px;
    text-align: center;
    line-height: 27px;
    border-radius: 2px;
    padding: 0;
    background: #30a5dd;
}

button:active {
    background: #30a5dd !important;
}

.pinjia-text {
    line-height: 27px;
    margin-right: 10px;
}

/* 我的订单 end */

/* 收藏夹 start */

.shoucang {
    padding: 0px 15px 0 15px;
    overflow: hidden;
}

.shoucang .goods {
    width: 100%;
    height: 240px;
    position: relative;
}

.shoucang .goodsList {
    width: 48%;
}

.shoucang .goodsList:nth-of-type(odd) {
    float: left;
}

.shoucang .goodsList:nth-of-type(even) {
    float: right;
}

.shoucang .goods img {
    display: block;
    width: 100%;
    height: 200px;
}

.sc-checkbox {
    background: url(../img/choose_false.png) no-repeat center center;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0;
    background-size: cover;
    -webkit-appearance: none;
}

.shoucang .goods span {
    position: absolute;
    bottom: 40px;
    width: 100%;
    height: 30px;
    line-height: 30px;
    background: #ddd;
    opacity: 0.8;
    color: #000;
    padding-left: 5px;
    font-size: 14px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.shoucang .goods b {
    font-size: 12px;
    float: left;
    height: 40px;
    line-height: 40px;
}

.shoucang .goods .text {
    font-size: 12px;
    width: 100%;
    height: 40px;
    text-align: right;
    line-height: 40px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.settlement {
    width: 100%;
    height: 50px;
    line-height: 50px;
    border-top: 1px solid #EDEDED;
    background: #FFF;
    position: fixed;
    bottom: 0;
    left: 0;
}

.settlement .sc-checkbox {
    float: left;
    position: static;
    margin: 15px 10px;
}

.settlement .set-btn {
    height: 100%;
    padding: 0 .8rem;
    color: #FFF;
    text-align: center;
    background: #ccc;
    float: right;
    margin-left: 5px;
}

/* 收藏夹 end */

/* 浏览记录 start */
.shoucang .time {
    display: block;
    margin-bottom: 10px;
}

/* 浏览记录 end */

/* 重置密码 start */
.authU {
    overflow: hidden;
    margin-top: 1rem;
}

.bgcolor {
    padding: 0 16px;
    background: #fff;
    border-top: 1px solid #e2e2e2;
    border-bottom: 1px solid #e2e2e2;
}

.authU li {
    width: 100%;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #e4e4e4;
    display: table;
    position: relative;
}

.authU li input {
    margin-bottom: 0;
    padding: 0;
    height: 50px;
    font-size: 14px;
    font-family: '微软雅黑';
    vertical-align: middle;
    border: 0;
}

.authU li:last-child {
    border-bottom: 0;
}

.authU li:last-child button {
    float: right;
    border: 1px solid #ddd;
    border-radius: 15px;
    width: 30%;
    height: 30px;
    margin-top: 10px;
}

.rest .quit_btn {
    margin-bottom: 20px;
}

/* 重置密码 end */

/* 地址管理 start */
.address-list {
    margin-top: 1rem;
    border-top: 1px solid #e2e2e2;
    background: #FFF;
}

.address-list .content {
    padding: .5rem .8rem;
    border-bottom: 1px dashed #e2e2e2;
}

.address-list .top {
    margin-bottom: .5rem;
    overflow: hidden;
}

.address-list .describe {
    font-size: .8rem;
    color: #666;
}

.address-list .operation {
    padding: .5rem .8rem;
    overflow: hidden;
}

.address-list .operation .fl {
    margin-top: .1rem;
    line-height: 1rem;
}

.moren-active {
    color: #f786a3;
}

.zdy-radio.active {
    background-image: url(../img/radio_true.png);
}

.zdy-radio {
    background: url(../img/choose_false.png) no-repeat center center;
    width: 1rem;
    height: 1rem;
    float: left;
    margin-right: .5rem;
    background-size: cover;
    -webkit-appearance: none;
    -moz-appearance: none;
    -o-appearance: none;
    appearance: none;
}

.address-list a {
    display: inline-block;
}

.address-list .operation-icon:last-child {
    margin-right: 0;
}

.address-list .operation-icon {
    width: 1.5rem;
    height: 1.5rem;
    margin: 0 .5rem;
    vertical-align: middle;
}

.addAddress {
    background: #FFF;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    padding: .5rem 0;
    text-align: center;
    border-top: 1px solid #E5E5E5;
}

.addAddress .address_btn {
    margin-bottom: 0;
    width: 80%;
    padding: .5rem 0;
    display: inline-block;
    color: #FFF;
    background: #30a5dd;
    text-align: center;
    border-radius: .2rem;
    border: none;
}

.addAddress img {
    width: 15px;
    height: 15px;
    vertical-align: text-top;
}

/* 地址管理 end */

/* 新建地址 start */
#addaddress .authU li label {
    width: 30%;
    margin: 11px 0
}

#addaddress .authU li input {
    width: 70%;
}

#addaddress .authU li .diqu {
    width: 70%;
    height: 30px;
    line-height: 30px;
    margin: 10px 0;
    float: left;
}

#addaddress .authU li .diqu p {
    float: left;
}

#addaddress .authU li .diqu i {
    background: url(../img/icon_arrow.png) no-repeat 98% center;
    display: block;
    width: 16px;
    height: 16px;
    float: right;
    margin-top: 7px;
}

.morenAddress {
    background: #fff;
    height: 50px;
    margin-top: .5rem;
    border-bottom: 1px solid #e2e2e2;
    border-top: 1px solid #e2e2e2;
    padding: 5px 16px;
}

.morenAddress .label-title {
    display: block;
    width: 40%;
    float: left;
}

.morenAddress .mui-switch {
    float: right;
    margin-top: 5px;
}

/* 新建地址 end */

.detail-title .mask {
    width: 100%;
    height: 50px;
    position: absolute;
    top: 114px;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 998;
    background-color: rgba(0, 0, 0, .3);
}

.detail-title .mask p {
    line-height: 50px;
    color: white;
    margin-left: 25px;
}

.content-text {
    border: 1px solid #CCCCCC;
    margin-top: 15px;
}

.content-text .left-box {
    float: left;
    width: 25%;
    height: 100px;
    padding: 5px;
}

.content-text .left-box img {
    width: 100%;
    height: 100%;
    display: block;
    vertical-align: center;
}

.content-text .right-box {
    float: right;
    width: 72%;
}

.content-text .right-box p {
    margin-top: 5px;
}

.main-text {
    margin-top: 15px;
}

.main-text .p-number {
    line-height: 30px;
    border: 1px solid #CCCCCC;
}

.main-text .left-box {
    width: 20%;
    margin: 5px;
}

.main-text .left-box img {
    width: 65px;
    height: 65px;
    display: block;
    vertical-align: center;
    margin-top: 10px;
}

.main-text .middle-box {
    margin-left: 5px;
    margin-top: 5px;
    width: 40%;
}

.main-text .middle-box span {
    font-size: 14px;
    color: #8f8f94;
}

.main-text .right-box {
    margin-top: 45px;
    margin-right: 15px;
}

.main-text .right-box strong {
    color: red;
    font-size: 14px;
}

.main-text ul li {
    border-bottom: 1px solid #CCCCCC;
    padding-bottom: 8px;
}

.totalmoney {
    line-height: 40px;
    margin-right: 15px;
    font-size: 14px;

}

.wrap-pay {
    padding: 10px;
}

.wrap-pay .box-list {
    line-height: 30px;
    border-bottom: 1px solid #CCCCCC;
}

.order-time {
    padding: 10px;
}

.order-time p {
    margin-top: 10px;
    line-height: 45px;
}

.select-style {
    border: 1px solid rgba(0, 0, 0, .2) !important;
    width: 25%;
    height: 40px;
}

.wrap-ul input {
    width: 70%;
}

.wrap-ul li {
    margin-top: 10px;
    height: 50px;
}

.clear-btn {
    display: block;
    margin: auto;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    background: #007aff;
    color: white;
    border-radius: 5px;
}
.my-span-style{
    display: inline-block;
    width: 145px;
    font-size: 12px;
}
.evaluate-left{
    width: 20%;
    height: 70px;

}
.evaluate-left img{
    display: block;
    width: 100%;
    height: 100%;
    vertical-align: middle;
}
.evaluate-content .pingfen{
   display: block;
    font-size: 15px;
    margin-left: 10px;
}
.evaluate-right button{
    width: 60px;
    height: 30px;
    font-size: 15px;
    margin-right: 10px;
    margin-top: 40px;
}
.ev-li{
    margin: 15px 0;

}
#evaluate textarea{
    width: 100%;
    min-height: 100px;
    margin-top: 15px;
    overflow-x: auto;
    padding: 10px;
}
