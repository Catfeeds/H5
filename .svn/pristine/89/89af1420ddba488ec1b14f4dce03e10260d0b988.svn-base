/* 重置密码 start */

.authU {
    overflow: hidden;
    margin-top: 1rem;
}

.bgcolor {
    padding: 0 16px;
    background: #fff;
    border-top: 1px solid #e2e2e2;
    border-bottom: 1px solid #e2e2e2;
}

.authU li {
    width: 100%;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #e4e4e4;
    display: table;
    position: relative;
}

.authU li input {
    margin-bottom: 0;
    padding: 0;
    height: 50px;
    font-size: 14px;
    font-family: '微软雅黑';
    vertical-align: middle;
    border: 0;
}

.authU li:last-child {
    border-bottom: 0;
}

.authU li:last-child button {
    float: right;
    border: 1px solid #ddd;
    border-radius: 15px;
    width: 30%;
    height: 30px;
    margin-top: 10px;
}

.rest .quit_btn {
    margin-bottom: 20px;
}

