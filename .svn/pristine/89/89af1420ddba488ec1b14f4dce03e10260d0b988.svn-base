/* 我的订单 end */

/* 收藏夹 start */

.shoucang {
    padding: 0px 10px;
    overflow: hidden;
}

.shoucang .goods {
    width: 100%;
    height: 215px;
    position: relative;
}

.shoucang .goodsList {
    width: 48%;        
    max-width: 175px;
    margin-bottom: 10px;
}

.shoucang .goodsList:nth-of-type(odd) {
    float: left;
}

.shoucang .goodsList:nth-of-type(even) {
    float: right;
}

.shoucang .goods img {
    display: block;
    width: 100%;
    height: 175px;
    border: 1px solid white;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
}

.sc-checkbox {
    background: url(../img/choose_false.png) no-repeat center center;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0;
    background-size: cover;
    -webkit-appearance: none;
}

.shoucang .goods .goodsName {
    position: absolute;
    bottom: 40px;
    width: 100%;
    height: 30px;
    line-height: 30px;
    background: rgba(107, 107, 107, 0.8);
    opacity: 0.8;
    color: white;
    padding-left: 5px;
    font-size: 12px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.shoucang .goods b {
    font-size: 12px;
    width: 100%;
    float: left;
    height: 40px;
    line-height: 40px;
    background: white;
    padding-left: 5px;
}

.shoucang .goods .text {
    font-size: 12px;
    width: 100%;
    height: 40px;
    text-align: right;
    line-height: 40px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.settlement {
    bottom: 0;
}

.shoucang .chechbox-product {
    position: absolute;
    right: 10px!important;
    top: 10px;
    width: 1.2rem;
    height: 1.2rem;
    background-color: #dddddd;
    border: solid 1px #dddddd;
    border-radius: 50%;
}

.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {
    color: #F52B9D;
}
#Collection input[type=checkbox]:before{
	    font-size: 25px;
}
.settlement input[type=checkbox]{
	vertical-align: middle;
    margin-left: 10px;
    position: initial;
}
.settlement.bottom_content{
	height: 48px!important;
    line-height: 48px!important;
}
