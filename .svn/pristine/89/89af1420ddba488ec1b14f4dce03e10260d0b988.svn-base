body {
    background: #f5f5f5;
    ;
    color: #333;
    font-size: 16px;
    margin-bottom: 80px;
}

[v-cloak] {
    display: none !important;
}

.mui-content {
    /*margin-top: 40px;*/
    background-color: #f5f5f5;
}

.mui-popup-button {
    color: #f786a3 !important;
}

a {
    color: #333;
}

.red {
    color: #F52B9D;
}

.user {
    height: auto;
    margin: 0 auto;
    position: relative;
    font-family: "微软雅黑";
}

.web-top-right {
    position: absolute;
    top: 0;
    right: 10px;
    font-size: 1rem;
    height: 100%;
    line-height: 44px;
}

.web-top-right a {
    display: inline-block;
    width: 20px;
    height: 20px;
    margin-top: 13px;
}

.web-top-right span {
    margin-right: 5px;
}

.user-top {
    position: fixed;
    width: 100%;
    height: 44px;
    background-color:rgb(255,255,255);
    opacity: 0;
    top: 0;
    left: 0;
    z-index: 3;
    box-shadow: 0 1px 6px #ccc;
    -webkit-box-shadow: 0 1px 6px #CCC;
}

.user-top-name {
    line-height: 44px;
    font-size: 16px;
    text-align: center;
    display: inline-block;
    width: 100%;
    opacity: 0;
    color: black;
}

.user .mui-icon-gear {
    position: fixed;
    line-height: 44px;
    text-align: center;
    width: 30px;
    height: 44px;
    margin-left: 90%;
    border-radius: 50%;
    color: white;
    z-index: 11;
}

.user .msg-icon {
    width: 100%;
    height: 100%;
}

.user .personal {
    position: relative;
    width: 100%;
    margin-top: -1px;
    padding: 50px 20px;
    background-size: 100% 100%;
    color: #fff;
    background: url(../img/bg.png);
}

.user .personal .number {
    float: left;
    margin-left: 15px;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    min-width: 0;
    line-height: 80px;
}

