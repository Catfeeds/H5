.user-main-list {
    background: #FFF;
    border-top: 1px solid #E5E5E5;
    margin-bottom: 10px;
}

.user-main-list li {
    width: 100%;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #E5E5E5;
    font-size: 14px;
}

.user-main-list li:nth-child(2) {
    border-bottom: 0;
}

.user-main-list li a {
    padding: 0px 10px;
    display: block;
    width: 100%;
    height: 100%;
}

.right-icon {
    color: #999;
    font-size: 14px;
    position: absolute;
    right: 10px;
    height: 50px;
    line-height: 50px;
}
.right-icon .icon{
	width: 14px;
	height: 14px;
}
.user-main-list img {
    width: 24px;
    height: 27px;
    margin: 12px 15px 0 0;
}

.viewOrder {
    color: #999;
    margin-right: 20px;
}

.order-item {
    overflow: hidden;
}

.order-box {
    width: 25%;
    text-align: center;
    padding: 10px 0;
    float: left;
}

.order-box a {
    display: block;
}

.order-box span {
    display: block;
    margin-top: 5px;
    font-size: 14px;
}

.order-box img {
    width: 30px;
    height: 30px;
    margin: 0;
}

.quit {
    margin: 20px auto;
}

.quit_btn {
    width: 80%;
    height: 40px;
    border: 1px solid #f786a3;
    background: #f786a3;
    text-align: center;
    display: block;
    color: white;
    font-size: 16px;
    margin: 20px 10%;
}

