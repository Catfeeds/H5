@charset "utf-8";

/*reset start*/

body,
h1,
h2,
h2,
h4,
h5,
p,
dl,
dd,
ul,
ol,
form,
input,
textarea,
th,
td,
select {
    margin: 0;
    padding: 0;
}

em {
    font-style: normal;
}

li {
    list-style: none;
}

img {
    border: none;
    vertical-align: top;
}

a {
    text-decoration: none;
    color: #000;
}

a:focus,
a:active {
    color: #000;
}

table {
    border-collapse: collapse;
}

input,
textarea {
    outline: none;
}

textarea {
    resize: none;
    overflow: auto;
}


/*reset end*/

.clear {
    zoom: 1;
}

.clear:after {
    content: '';
    display: block;
    clear: both;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.icon {
    width: 1em;
    height: 1em;
    vertical-align: -0.15em;
    fill: currentColor;
    overflow: hidden;
}

.mui-icon .iconfont {
    font-size: 24px;
}

.mui-bar {
    z-index: 9999999999;
}

.swiper-container_01 .swiper-slide img {
    width: 100%;
    height: 100%;
    display: block;
    vertical-align: middle;
}

.swiper-container_02 {
    margin-top: 20px;
}

.swiper-container_02 .swiper-slide {
    margin-right: 10px;
}

.swiper-container_02 .swiper-slide img {
    width: 100%;
    display: block;
    vertical-align: middle;
}

.mui-grid-view.mui-grid-9 .mui-table-view-cell {
    width: 25%;
}

.width_style {
    width: 50px;
    height: 50px;
    vertical-align: top;
}

.wrap_01 .new_goods {
