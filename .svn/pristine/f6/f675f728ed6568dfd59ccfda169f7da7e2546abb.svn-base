<script>
    $(function () {
        //点击收起筛选条件
        $('.my-fold').click(function () {
            $('.filterContent').toggleClass('active');
        })

        //筛选li的点击事件
        $('.info li').click(function () {
            if ($(this).find('.common-style').hasClass('active')) {
                $(this).find('.common-style').removeClass('active');
            } else {
                $(this).find('.common-style').toggleClass('active');
            }

        })
        $('#Shape li').click(function () {
            if ($(this).hasClass('active')) {
                $(this).removeClass('active');
            } else {
                $(this).toggleClass('active');
            }

        })


        //打印结果上面的点击事件
        $('.orderby').click(function () {
            $(this).find('i').toggleClass('glyphicon-menu-up');
        });

    })
</script>
<script>
    new Vue({
        el: '#single-diamond',
        data: {
            diamondattrList: [],//钻石属性列表
            inputValue01: 0.3,
            inputValue02: 1000,
            whiteD: true,
            colorD: false,
            diamondData: []
        },
        created: function () {
            var _this = this;
            //钻石初始化数据
            var paramsData = {
                type: 0,
                page: 1,
                orderby: 'supply_gtype asc',
                goods_attr_filter: '',
            }
            this.commonFN(paramsData);
            //钻重按钮
            // var weight=
        },
        methods: {
            //白钻按钮
            whiteDiamond: function () {
                this.whiteD = true;
                this.colorD = false;

            },
            //彩钻按钮
            colorDiamond: function () {
                this.whiteD = false;
                this.colorD = true;
            },
            //获取钻石数据公共函数
            commonFN: function (params) {
                var _this = this;
                axios({
                    url: __apiurl + '/goods/diamond',
                    method: 'GET',
                    params: params,
                    headers: getHeader(false),
                }).then(function (res) {
                    _this.diamondData = res.data.data;
                }).catch(function (error) {
                   alert(error)
                })
            },
            //搜索确认按钮
            searchConfirm:function(){
                var str='';

            }
        }
    })
</script>