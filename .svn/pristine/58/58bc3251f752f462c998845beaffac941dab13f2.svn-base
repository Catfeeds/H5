<script>
    //判断是否在数组里面
    function in_array(search, array) {
        for (var i in array) {
            if (array[i] == search) {
                return true;
            }
        }
        return false;
    }

    var vm = new Vue({
        el: '#diamond',
        data:{
                isShowTitle: true,
                isShowTitle01: false,
                isOk: false,
                isOk01: false,
                isShow: true,
                isShow01: false,
                detailsShow: false,
                diamondList: [], //白钻数据
                diamondColorList: [], //彩钻数据
                goodsId: '',
                papvals: [],
                attrObj: [],
                addList: '',
                page: 1,
                cartnum: 0,
                listObj: '',
                token: '',
                showImg: false,
                str: '',
                goodsIdList: [],
                diamondtype:0, //当前页面显示的钻石类型

        },
        filters: {
            //保留2位小数点过滤器 不四舍五入
            number:function(value) {
                var toFixedNum = Number(value).toFixed(3);
                var realVal = toFixedNum.substring(0, toFixedNum.toString().length - 1);
                return realVal;
            }
        },
        created:function() {
            /*获取token*/
            this.token = getToken();
            this.id = getUrlParam("category_id");
            //初始数据
            this.diamondtype = 0;
            var params = {attr:'type:'+this.diamondtype,page:1};
            this.getlist(params);

        },
        methods: {
            //获取钻石数据
            getlist:function(params) {
                var _this = this;
                axios.get(__apiurl + '/goods/diamond', {
                    params: {
                        goods_attr_filter: params.attr,
                        page: params.page
                    }
                }).then(function (response){
                    _this.diamondList = response.data.data.data;
                    _this.page = response.data.data.page;
                }).catch(function (error) {
                    mui.alert(error);
                });
            },

            clickImg:function() {

                if (this.diamondtype == 0) {
                    this.isOk = true;
                } else if (this.diamondtype == 1) {
                    this.isOk01 = true;
                }
            },
            //遮罩层
            w_mask:function() {
                this.isOk = false;
                this.isOk01 = false;
            },
            //白钻按钮
            toggleClick:function() {
                this.isShow = true;
                this.isShow01 = false;
                this.reset(); //清空属性
                this.page = 1;
                this.diamondtype = 0;
                var params = {attr:'type:'+this.diamondtype,page:this.page};
                this.getlist(params);
            },
            //彩钻按钮
            toggleClick01:function() {
                this.isShow01 = true;
                this.isShow = false;
                this.reset(); //清空属性
                this.page = 1;
                this.diamondtype = 1;
                var params = {attr:'type:'+this.diamondtype,page:this.page};
                this.getlist(params);
            },
            //获取点击的属性
            getattr:function(){
                this.str = '';
                for (var index in this.attrObj) {
                    for (var i in this.attrObj[index]) {
                        var tmp = this.attrObj[index][i].substr(1);
                        if (tmp) {
                            this.str += i + ':' + tmp + ';';
                        }
                    }
                }
                return this.str;
            },
            //白钻彩钻搜索确定
            confirm:function() {
                var params = {attr:this.getattr()+'type:'+this.diamondtype,page:1};
                this.getlist(params);
                this.isOk = false; //关闭这个搜索页
                this.isOk01 = false;
            },
 
            //重置
            reset:function() {
                var _this = this;
                $('li').each(function (i, el) {
                    if ($(el).hasClass('active')) {
                        $(el).removeClass('active');
                        var papval = $(el).parent().prev().attr('value');
                        for (var index in _this.attrObj) {
                            for (var i in _this.attrObj[index]) {
                                if (i == papval) {
                                    var temp = [];
                                    temp[papval] = '';
                                    _this.attrObj[index] = temp;
                                }
                            }
                        }
                    }
                })
                _this.str = '';
            },
            
            buttom_alert:function() {
                this.detailsShow = false;
            },
            /*点击每条LI  弹出一个框*/
            isShowAlert:function(obj, id) {
                //收藏数据
                var _this = this;
                axios({
                    method: 'POST',
                    url: __apiurl + '/user/getUserGoodsCollectionList',
                    headers: {
                        Accept: "application/json; charset=utf-8",
                        Token: this.token
                    }
                }).then(function (res) {

                    var aa = [];
                    if (res.data.status == 100) {
                        this.dataList = res.data.data.data;
                        for (var i in this.dataList) {
                            aa.push(this.dataList[i].goods_id);
                            _this.goodsIdList = aa;
                        }
                    }
                }).catch(function (error) {
                    mui.alert(error)
                })

                _this.goodsId = id;
                _this.detailsShow = true;
                axios({
                    url: __apiurl + '/goods/getdiamondinfo',
                    method: "get",
                    params: {
                        id: id
                    }
                }).then(function (res) {
                    _this.listObj = res.data.data;
                    _this.listObj.goods_price = obj.goods_price;

                    //收藏判断
                    if (in_array(_this.goodsId, _this.goodsIdList)) { //判断this.id是否在数组中
                        $('.my-span-background').addClass('active');
                    }
                }).catch(function (error){
                    mui.alert(error.status)
                })


                //购物车数据  只取购物车总数
                axios({
                    method: 'GET',
                    url: __apiurl + '/cart/index',
                    headers: {
                        Accept: "application/json; charset=utf-8",
                        Token: this.token
                    }
                }).then(function (res){
                    if(res.status==200){
                        _this.cartnum=res.data.data.total_num;
                        // console.log( this.cartnum)
                    }
                    if(res.status==1016){
                        _this.cartnum=0
                    }
                })
            },
            //添加购物车
            addcart:function() {
                this.detailsShow = false;
                __checkLogin();
                axios({
                    method: 'post',
                    headers: {
                        Accept: "application/json; charset=utf-8",
                        Token: this.token
                    },
                    data: {
                        goods_id: this.goodsId,
                        goods_num: 1,
                        type: 0
                    },
                    url: __apiurl + '/Cart/addToCart',
                }).then(function (res) {
                   mui.toast(res.data.msg);
                }).catch(function (error) {
                    mui.alert(error)
                })

            },
            // 匹配戒托
            pipei:function() {
                this.detailsShow = false;
                var _this = this;
                __checkLogin();
                window.location.href = "{:url('goods/custom')}" + '?diamond_id=' + _this.goodsId;
            },

            // 速订购直接提交订单页
            suAddCart:function() {
                this.detailsShow = false;
                __checkLogin();
                axios({
                    method: 'post',
                    headers: {
                        Accept: "application/json; charset=utf-8",
                        Token: this.token
                    },
                    data: {
                        goods_id: this.goodsId,
                        goods_num: 1,
                        type: 1
                    },
                    url: __apiurl + '/Cart/addToCart',
                }).then(function (res)  {
                    if (res.data.status == 100) {
                        window.location.href = "{:url('order/confirm')}";
                    }
                }).catch(function(error) {
                    mui.alert(error)
                })
                // }
            },
            choseli:function(obj) {
                var sonval = obj.attr('value');
                var papval = obj.parent().prev().attr('value');
                if (obj.hasClass('active')) {
                    obj.removeClass('active');
                    for (var index in this.attrObj) {
                        for (var i in this.attrObj[index]) {
                            if (papval == i) {
                                var tmp = this.attrObj[index][i].replace("," + sonval, "");
                                if (tmp) {
                                    this.attrObj[index][i] = tmp;
                                } else {
                                    var temp = [];
                                    temp[papval] = '';
                                    this.attrObj[index] = temp;
                                    // delete(this.attrObj[index]);
                                }
                            }
                        }
                    }
                } else {
                    obj.addClass('active');
                    if (this.papvals.indexOf(papval) == -1) { //判断是否选中了该父属性
                        this.papvals.push(papval);
                        var str = [];
                        str[papval] = ',' + sonval;
                        this.attrObj.push(str);
                    } else {
                        for (var index in this.attrObj) { //遍历父元素数组
                            for (var i in this.attrObj[index]) {
                                if (papval == i) {
                                    //改变价格li和钻重li的单选颜色
                                    if (i == 'Priceinterval' || i == 'Weightinterval') {
                                        this.attrObj[index][i] = ',' + sonval;
                                        obj.siblings().removeClass('active');
                                    } else {
                                        if (sonval != this.attrObj[index][i]) {
                                            this.attrObj[index][i] = this.attrObj[index][i] + ',' + sonval;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
                console.log(this.attrObj);
            },
            //top 上购物车按钮点击事件
            cart:function() {
                window.location.href = "{:url('cart/index')}";
            },
            //收藏按钮
            collect:function() {
                __checkLogin();
                if ($('.my-span-background').hasClass('active')) {
                    axios({
                        method: 'POST',
                        url: __apiurl + '/user/delusergoodscollection',
                        headers: {
                            Accept: "application/json; charset=utf-8",
                            Token: this.token
                        },
                        data: {
                            goods_id: this.goodsId
                        }
                    }).then(function (res) {
                        if (res.data.status == 100) {
                           mui.toast(res.data.msg);
                           $('.my-span-background').removeClass('active');
                        }
                    }).catch(function (error) {
                        mui.alert(error)
                    })
                } else {
                    axios({
                        method: 'POST',
                        url: __apiurl + '/user/addusergoodscollection',
                        headers: {
                            Accept: "application/json; charset=utf-8",
                            Token: this.token
                        },
                        data: {
                            goods_id: this.goodsId
                        }
                    }).then(function (res)  {
                        if (res.data.status == 100) {
                           mui.toast(res.data.msg);
                           $('.my-span-background').addClass('active');
                        }
                    }).catch(function (error)  {
                        console.log(error)
                    })
                }
            }
        }
    });
    //收藏按钮
    $('.collect').click(function () {
        vm.collect();
    });
    /*点击li*/
    $('.content_list ul li').click(function () {
        vm.choseli($(this));
    })
    //--------------上拉加载更多---------------
    //获取滚动条当前的位置
    function getScrollTop() {
        var scrollTop = 0;
        if (document.documentElement && document.documentElement.scrollTop) {
            scrollTop = document.documentElement.scrollTop;
        } else if (document.body) {
            scrollTop = document.body.scrollTop;
        }
        return scrollTop;
    }

    //获取当前可视范围的高度
    function getClientHeight() {
        var clientHeight = 0;
        if (document.body.clientHeight && document.documentElement.clientHeight) {
            clientHeight = Math.min(document.body.clientHeight, document.documentElement.clientHeight);
        } else {
            clientHeight = Math.max(document.body.clientHeight, document.documentElement.clientHeight);
        }
        return clientHeight;
    }

    //获取文档完整的高度
    function getScrollHeight() {
        return Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
    }


    //滚动事件触发
    window.onscroll = function () {
        if (getScrollTop() + getClientHeight() == getScrollHeight()) {
            setTimeout(function () {
                vm.showImg = true;
            }, 1000)
            vm.page++;
            /*白钻数据*/
            axios.get(__apiurl + '/goods/diamond', {
                params: {
                    goods_attr_filter: vm.str + 'type:'+vm.diamondtype,
                    page: vm.page
                }
            }).then(function (response) {
                vm.showImg = false;
                var diamondList01 = response.data.data.data;
                vm.diamondList = vm.diamondList.concat(diamondList01)
            }).catch(function (error){
                mui.alert(error);
            });
        }
    }
    //-----------------结束--------------------
</script>

